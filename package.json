{
  "name": "@cheprasov/jsbt",
  "version": "1.3.1",
  "author": "Alexander Cheprasov",
  "description": "JavaScript Binary Transfer (JSBT) â€“ graph-aware binary serialization for JavaScript runtimes.",
  "keywords": [
    "JSBT",
    "serialization",
    "binary",
    "javascript"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cheprasov/ts-jsbt.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap --minify --clean --target es2020 --out-dir dist",
    "test": "jest",
    "lint": "eslint src",
    "check-esm": "node --input-type=module -e \"import { JSBT } from './dist/index.js'; console.log(JSBT)\"",
    "check-cjs": "node -e \"const { JSBT } = require('./dist/index.cjs'); console.log(JSBT)\"",
    "check": "npm run check-esm & npm run check-cjs"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2"
  }
}
